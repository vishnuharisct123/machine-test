/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@app/*":["src/app/*"],
      "@env/*":["src/environments/*"],
      "@the/*":["src/theme/*"],
      "@shared/*":["src/app/shared/*"],
      "@core/*":["src/app/core/*"],
      "@util/*":["src/app/util/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],

}
